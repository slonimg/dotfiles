function warehouse_start() {
    local USER="$AWS_PROFILE"

    user-tp
    aws ec2 start-instances --instance-ids i-02a85cb6436811a08

    [[ "$USER" == "grubhub" ]] && user-gh
}

function warehouse_stop() {
    local USER="$AWS_PROFILE"
    user-tp
    aws ec2 stop-instances --instance-ids i-02a85cb6436811a08

    [[ "$USER" == "grubhub" ]] && user-gh
}

function warehouse_connect() {
    local USER="$AWS_PROFILE"
    user-tp
    ssh -i ~/.keys/yarden.pem ubuntu@10.0.1.123

    [[ "$USER" == "grubhub" ]] && user-gh
}

