function get_params() {
    garcli fig get-all-params -e preprod --service grouter --system managed_delivery --version $1 --username gslonim@grubhub.com
}

function set_param() {
    garcli fig set-param -e preprod --system managed_delivery  --service grouter --version $1 --param $2 --value $3 --username gslonim@grubhub.com --runtime
}
